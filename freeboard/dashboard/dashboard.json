                    },
                    "type": "text_widget"
                },
                {
                    "settings": {
                        "max_value": 100,
                        "min_value": 0,
                        "title": "Humidity",
                        "units": "%",
                        "value": "datasources[\"Bedroom\"][\"feeds\"][1][\"field5\"]"
                    },
                    "type": "gauge"
                }
            ],
            "width": 1
        }
    ],
    "plugins": [],
